{
  "dataset": "hcrl_sa",
  "model_type": "vgae",
  "scale": "large",
  "seed": 42,
  "vgae": {
    "hidden_dims": [
      480,
      240,
      48
    ],
    "latent_dim": 48,
    "heads": 4,
    "embedding_dim": 32,
    "dropout": 0.15
  },
  "gat": {
    "hidden": 48,
    "layers": 3,
    "heads": 8,
    "dropout": 0.2,
    "embedding_dim": 16,
    "fc_layers": 3
  },
  "dqn": {
    "hidden": 576,
    "layers": 3,
    "gamma": 0.99,
    "epsilon": 0.1,
    "epsilon_decay": 0.995,
    "min_epsilon": 0.01,
    "buffer_size": 100000,
    "batch_size": 128,
    "target_update": 100
  },
  "training": {
    "lr": 0.002,
    "max_epochs": 300,
    "batch_size": 4096,
    "patience": 100,
    "weight_decay": 0.0001,
    "gradient_clip": 1.0,
    "precision": "16-mixed",
    "safety_factor": 0.55,
    "gradient_checkpointing": true,
    "use_teacher_cache": true,
    "clear_cache_every_n": 100,
    "offload_teacher_to_cpu": false,
    "optimize_batch_size": true,
    "memory_estimation": "measured",
    "accumulate_grad_batches": 1,
    "save_top_k": 1,
    "monitor_metric": "val_loss",
    "monitor_mode": "min",
    "log_every_n_steps": 50,
    "test_every_n_epochs": 5,
    "deterministic": false,
    "cudnn_benchmark": true,
    "use_scheduler": false,
    "scheduler_type": "cosine",
    "scheduler_t_max": -1,
    "scheduler_step_size": 50,
    "scheduler_gamma": 0.1,
    "curriculum_start_ratio": 1.0,
    "curriculum_end_ratio": 10.0,
    "difficulty_percentile": 75.0,
    "use_vgae_mining": true,
    "difficulty_cache_update": 10,
    "curriculum_memory_multiplier": 1.0,
    "log_teacher_student_comparison": true
  },
  "auxiliaries": [],
  "fusion": {
    "episodes": 500,
    "max_samples": 150000,
    "max_val_samples": 30000,
    "episode_sample_size": 20000,
    "training_step_interval": 32,
    "gpu_training_steps": 16,
    "lr": 0.001,
    "alpha_steps": 21
  },
  "preprocessing": {
    "window_size": 100,
    "stride": 100
  },
  "schema_version": "1.0.0",
  "experiment_root": "experimentruns",
  "device": "cuda",
  "num_workers": 8,
  "mp_start_method": "spawn",
  "run_test": true
}